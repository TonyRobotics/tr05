<?xml version="1.0"?>
<launch>
    <node pkg="tf" type="static_transform_publisher" name="broadcaster_us0"
    args="-0.145 -0.035 0.06 3.14 0 0 /base_link /ultrasound0 100" />
    <node pkg="tf" type="static_transform_publisher" name="broadcaster_us1"
    args="-0.145 0.035 0.06 3.14 0 0 /base_link /ultrasound1 100" />
    <node pkg="tf" type="static_transform_publisher" name="broadcaster_us2"
    args="0.145 -0.035 0.06 0 0 0 /base_link /ultrasound2 100" />
    <node pkg="tf" type="static_transform_publisher" name="broadcaster_us3"
    args="0.145 0.035 0.06 0 0 0 /base_link /ultrasound3 100" />
  
    <arg name="port" default="/dev/ultrasound"/>
    <arg name="baud" default="57600"/>
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="$(arg port)"/>
        <param name="baud" value="$(arg baud)"/>
    </node>
  
</launch>
